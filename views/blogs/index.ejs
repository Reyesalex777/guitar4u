<%- include('../partials/header') %>
    <table>
        <thead>

            <tr>
                <th>Title</th>
                <th>Blog</th>
            </tr>
        </thead>
        <tbody>
            <% blogs.forEach(function(b) { %>
                <tr>
                    <td>
                        <%= b.title %>
                    </td>
                    <td>
                        <%= b.blog %>
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>

    <%- include('../partials/footer') %>